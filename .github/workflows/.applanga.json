{
    "app": {
      "pull": {
        "target": [
          {
            "file_format": "react_simple_json", 
            "exclude_languages": ["en"],
            "path": "./react_json_sample/<language>/translations.json",
            "tag": "app:translations.json"
          }
        ]
      }, 
      "push": {
        "source": [
          {
            "language": "en",
            "file_format": "react_simple_json", 
            "path": "./react_json_sample/en/translations.json",
            "tag": "app:translations.json"
          }
        ]
      }
    }
  }